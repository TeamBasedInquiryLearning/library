<section xml:id="TR3" xmlns:xi="http://www.w3.org/2001/XInclude">
    <title>Trigonometric Ratios (TR3) </title>
    <objectives>
    <ul>
      <li>
        <xi:include href="outcomes/03.ptx" />
      </li>
    </ul>
