<diagram dimensions="(300,300)" margins="5" xmlns="https://prefigure.org">
  <definition>f(x) = (x+1)*(x-3)</definition>
  <coordinates bbox="(-5,-5,5,5)">
    <grid/>
    <axes/>
    <graph function="f" />
    <label anchor="(3,-2.5)" color="blue"><m>g(x)</m></label>
  </coordinates>
  <annotations>
    <annotation ref="figure" text="The coordinate plane with a graph">
      <annotation ref="f" sonify="yes" text="The graph of a parabola crossing the x axis at -1 and 3, and the y axis at -3"/>
    </annotation>
  </annotations>
</diagram>