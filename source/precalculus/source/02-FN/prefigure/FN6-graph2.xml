<diagram dimensions="(300,300)" margins="5" xmlns="https://prefigure.org">
  <definition>f(x)=1/2*x^3</definition>
  <coordinates bbox="(-2,-4,2,4)">
    <grid-axes/>
    <graph at="f" function="f"/>
  </coordinates>
  <annotations>
    <annotation ref="figure"
                text="The graph of a function">
      <annotation ref="f" sonify="yes" text="A strictly increasing function"/>
    </annotation>
  </annotations>
</diagram>