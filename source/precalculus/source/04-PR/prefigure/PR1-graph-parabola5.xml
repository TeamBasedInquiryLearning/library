<diagram dimensions="(300,300)" margins="20" xmlns="https://prefigure.org">
  <definition>width=6</definition>
  <definition>f(x)=(x-2)^2-3</definition>
  <coordinates bbox="(-width,-width,width,width)">
    <grid-axes spacings="(-width,1,width),(-width,1,width)"/>
      <axes >
        <ylabel alignment="north">Graph C</ylabel>
      </axes>
    <graph at="f" function="f"/>
  </coordinates>
  <annotations>
    <annotation ref="figure" text="A graph of a parabola labeled Graph C">
        <annotation ref="f"  sonify="yes" text="A parabola that slopes down to the point 2 comma -3 and then slopes up"/>
    </annotation>
  </annotations>
</diagram>