<diagram dimensions="(300,300)" margins="5" xmlns="https://prefigure.org">
  <definition>width=6</definition>
  <definition>f(x)=(x+3)^2+2</definition>
  <coordinates bbox="(-width,-width,width,width)">
    <grid-axes spacings="(-width,1,width),(-width,1,width)"/>
      <axes >
        <ylabel alignment="north">Graph B</ylabel>
      </axes>
    <graph at="f" function="f"/>
  </coordinates>
  <annotations>
    <annotation ref="figure" text="A graph of a parabola labeled Graph B">
        <annotation ref="f"  sonify="yes" text="A parabola that slopes down to the point -3 comma 2 and then slopes up"/>
    </annotation>
  </annotations>
</diagram>