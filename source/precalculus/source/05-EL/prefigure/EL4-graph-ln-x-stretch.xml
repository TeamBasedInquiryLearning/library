<diagram dimensions="(300,300)" margins="5" xmlns="https://prefigure.org">
  <definition>f(x)=3*log(x)</definition>
  <coordinates bbox="(-5,-5,10,10)">
    <grid-axes/>
    <graph at="function" function="f"/>
    <group at="points">
    <point p="(1,0)" at="p1"/>
    <point p="(e,3)" at="p2"/>
    </group>
    <line at="asymptote" endpoints="((0,-5),(0,5))" stroke="red" infinite = "yes" dash="9 9"/>
  </coordinates>
  <annotations>
    <annotation ref="figure" text="Points on the graph of the function y equals 3 ln of x">
      <annotation ref="function" text="The function y equals 3 ln of x" sonify="yes"/>
      <annotation ref="asymptote" text="The vertical asymptote x equals 0"/>
      <annotation ref="points" text="Selected points on the graph of y equals 3 ln of x">
        <annotation ref="p1" text="The point 1 comma 0"/>
        <annotation ref="p2" text="The point 2.72 comma 3"/>
      </annotation>
    </annotation>
  </annotations>
</diagram>