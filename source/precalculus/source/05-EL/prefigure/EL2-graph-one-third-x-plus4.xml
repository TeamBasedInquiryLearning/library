<diagram dimensions="(300,300)" margins="5" xmlns="https://prefigure.org">
  <definition>f(x)=(1/3)^x+4</definition>
  <coordinates bbox="(-5,-5,5,10)">
    <grid-axes/>
    <graph at="function" function="f"/>
    <group at="points">
    <point p="(-1,7)" at="p1"/>
    <point p="(0,5)" at="p2"/>
    </group>
    <line at="asymptote-h" endpoints="((-5,4),(5,4))" stroke="red" infinite = "yes" dash="9 9"/>
  </coordinates>
    <annotations>
    <annotation ref="figure" text="The graph of the function y equals one third to the x plus 4, selected points, and its asymptote">
      <annotation ref="function" text="The function y equals one third to the x plus 4" sonify="yes"/>
      <annotation ref="asymptote-h" text="The horizontal asymptote y equals 4"/>
      <annotation ref="points" text="Selected points on the graph of y equals one third to the x plus 4">
        <annotation ref="p1" text="The point -1 comma 7"/>
        <annotation ref="p2" text="The point 0 comma 5"/>
      </annotation>
    </annotation>
  </annotations>
</diagram>