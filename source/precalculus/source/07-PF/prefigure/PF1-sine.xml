<diagram dimensions="(300,300)" margins="5" xmlns="https://prefigure.org">
  <definition>f(x)=sin(x)</definition>
  <coordinates bbox="(-2*pi-0.2,-1.1,4*pi+0.2,1.1)">
    <grid-axes spacings="(-2*pi,pi/2,4*pi),(-1,0.25,1)" h-pi-format="yes" hlabels="(-2*pi,pi,4*pi)"/>
    
    <graph at="f" function="f" />
  </coordinates>
  <annotations>
    <annotation ref="figure" text="The sine curve from -2 pi to 4 pi.">
      <annotation ref="f" sonify="yes" text="The curve sine of x"/>
    </annotation>
  </annotations>
</diagram>