<diagram dimensions="(300,300)" margins="50" xmlns="https://prefigure.org">
  <definition>f(x)=sin(x)</definition>
  <coordinates bbox="(0,-1.1,2*pi+0.2,1.1)">
    <grid-axes spacings="(0,pi/2,2*pi),(-3,0.25,3)" h-pi-format="yes" />
    <graph at="f" function="f"  domain="(0,2*pi)"/>
    <label p="(pi,1.2)"><m>f(x)=\sin(x)</m></label>
  </coordinates>
  <annotations>
    <annotation ref="figure" text="The sine curve ">
      <annotation ref="f" sonify="yes" text="The curve sine of x between 0 and 2 pi"/>
    </annotation>
  </annotations>
</diagram>