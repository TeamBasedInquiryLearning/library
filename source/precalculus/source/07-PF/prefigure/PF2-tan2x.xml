<diagram dimensions="(300,300)" margins="30" xmlns="https://prefigure.org">
  <definition>f(x)=tan(2*x)</definition>
  <coordinates bbox="(-2*pi-0.2,-3.1,2*pi+0.2,3.1)">
    <grid-axes spacings="(-2*pi,pi/2,2*pi),(-3,1,3)" h-pi-format="yes"  vlabels="(-3,1,3)"/>
    <graph at="f" function="f" />

    <group at="asymptotes">
      <repeat parameter="i=-3..4">
        <line at="L" endpoints="((-pi/4+i*pi/2,0),(-pi/4+i*pi/2,1))" dash="9 9" stroke="red" infinite="yes"/>
        </repeat>
    </group>
  </coordinates>
  <annotations>
    <annotation ref="figure" text="The graph tangent of 2 times x and its asymptotes">
      <annotation ref="f" sonify="yes" text="The graph of tangent of 2 times x"/>
      <annotation ref="asymptotes" text="The vertical asymptotes"/>
    </annotation>
  </annotations>
</diagram>