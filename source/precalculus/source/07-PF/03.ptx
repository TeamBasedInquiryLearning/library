            </row>
            <row bottom="minor">
              <cell><m>0</m></cell>
              <cell><m>0</m></cell>
            </row>
            <row bottom="minor"> 
              <cell> <m>\dfrac{1}{2}</m> </cell>
              <cell> <m>\dfrac{\pi}{6}</m></cell>               
            </row>
            <row bottom="minor"> 
              <cell> <m>\dfrac{\sqrt{2}}{2}</m> </cell>
              <cell> <m>\dfrac{\pi}{4}</m></cell>               
            </row>
            <row bottom="minor"> 
