              <cell> </cell>               
            </row>
            <row bottom="minor">
              <cell><m>0</m></cell>
              <cell></cell>
            </row>
            <row bottom="minor"> 
              <cell> <m>\dfrac{1}{2}</m> </cell>
              <cell> </cell>               
            </row>
            <row bottom="minor"> 
              <cell> <m>\dfrac{\sqrt{2}}{2}</m> </cell>
              <cell> </cell>               
            </row>
            <row bottom="minor"> 
              <cell> <m>\dfrac{\sqrt{3}}{2}</m> </cell>
              <cell> </cell>               
            </row>
            <row bottom="minor">
