    <task>
      <statement>
        <p>
          <m>f(x)=\dfrac{3}{2}\cos(x+\pi)</m>
        </p>
      </statement>
      <answer>
        <p>
          Amplitude: <m>\dfrac{3}{2}</m>
        </p>
        <p>
          Period: <m>2\pi</m>
        </p>
        <p>
          Phase shift: <m>-\pi</m> (or <m>\pi</m> to the left)
        </p>
      </answer>
    </task>
    <task>
      <statement>
        <p>
          <m>f(x)=3\sin(2x-\pi)</m>
        </p>
      </statement>
      <answer>
        <p>
          Amplitude: <m>3</m>
        </p>
        <p>
          Period: <m>\pi</m>
        </p>
        <p>
          Phase shift: <m>\dfrac{\pi}{2}</m> (or <m>\dfrac{\pi}{2}</m> to the right)
        </p>
      </answer>
    </task>
    <task>
      <statement>
        <p>
          <m>f(x)=4\sin\left(3x+\dfrac{\pi}{2}\right)</m>
        </p>
