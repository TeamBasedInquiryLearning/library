<diagram dimensions="(300,300)" margins="5" xmlns="https://prefigure.org">
<definition>A=(0,0)</definition>
<definition>B=(6,0)</definition>
<definition>C=(2,2)</definition>
<definition>a=pi/3</definition>
<definition>r=2/cos(a)</definition>
<definition>b=(2+r*cos(3*pi/2+a),2+r*sin(3*pi/2+a))</definition>
  <coordinates bbox="(-3,-3,7,7)">
    <line at="AC" endpoints="(A,C)" stroke="black" rotate="-45" offset="(0,5)" tactile-offset="(0,20)"><m>b</m></line>
    <line at="BC" endpoints="(C,b)" stroke="blue" rotate="30"  offset="(0,5)"><m>a</m></line>
    <line at="AB" endpoints="(A,B)" stroke="black" dash="9 9" />
    <point p="A" alignment="southwest"><m>A</m></point>
    <point p="C" alignment="north"><m>C</m></point>
    <angle-marker at="beta" points="(B,A,C)" thickness="1"/>
    <arc at="circle" center="C" radius="r" range="(190,350)" stroke="blue" dash="9 9" thickness="1" />
  </coordinates>
<annotations>
  <annotation ref="figure" text="An incomplete triangle diagram">
    <annotation ref="AC" text="A line segment from A to C is labeled b "/>
    <annotation ref="AB" text="The side extending horizontally from A is dashed."/>
    <annotation ref="BC" text="A blue line segment out straight down from C is labeled a"/>
    <annotation ref="circle" text="A part of a dashed blue circle centered at C with radius of length a. The radius is so large it meets the dashed line in exactly one place."/>
  </annotation>
 </annotations>
</diagram>