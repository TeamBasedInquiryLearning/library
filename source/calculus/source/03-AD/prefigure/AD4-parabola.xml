<diagram dimensions="(300,300)" margins="35" xmlns="https://prefigure.org">
  <definition> f(x) = -16*x^2+32*x+48</definition>
  <coordinates bbox="(-0.1,-1,3.2,75)">
    <grid-axes >
      <xlabel alignment="east"><m>t</m></xlabel>
      <ylabel alignment="north"><m>s(t)</m></ylabel>
    </grid-axes>
    <graph at="f" function="f" domain="(0,3)"/>
  </coordinates>

  <annotations>
    <annotation ref="figure" text="The graph of a downward facing parabola">
      <annotation ref="f" sonify="yes" text="A parabola beginning at 0 comma 48, increasing to a maximum at t equals 1, then descending to the point 3 comma 0"/>
    </annotation>
  </annotations>
</diagram>