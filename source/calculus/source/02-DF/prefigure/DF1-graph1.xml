<diagram dimensions="(300,300)" margins="35"  xmlns="https://prefigure.org">
  <definition>g(x)=1/2*x^2-2</definition>
  <coordinates bbox="(-0.1,-2.2,4.1,6.2)" >
    <grid-axes/>
    <graph at="g" function="g" />
    <label p="(2,6.5)"><m>g(x)</m></label>
  </coordinates>
  <annotations>
    <annotation ref="figure" text="The graph of g of x">
      <annotation ref="g" sonify="yes" text="The starts at -2 comma 0, and slopes up at an increasing rate, passing through 2 comma 0"/>
    </annotation>
  </annotations>
</diagram>