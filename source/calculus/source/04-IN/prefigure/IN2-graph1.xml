<diagram dimensions="(300,300)" margins="35" xmlns="https://prefigure.org">
  <definition>f(x) = 1/2*(x-1)^3 + 2</definition>
  <coordinates bbox="(-0.1,-0.1,2.1,2.75)">
    <grid-axes spacings="((0,0.2,3),(0,0.5,10))" hlabels="(0,1,2)" vlabels="(0,0.5,10)">
      <xlabel alignment="east">hrs</xlabel>
      <ylabel alignment="north">mph</ylabel>
    </grid-axes>
    <graph at="f" function="f"/>
    <label anchor="(1,1.5)" alignment="north"><m>y=v(t)</m></label>
  </coordinates>

  <annotations>
    <annotation ref="figure" text="A graph of a curve labeled y equals v of t">
      <annotation ref="f" sonify="yes" text="A curve beginning at 0 comma 1.5, increasing and then flattening out around 1 comma 2 before increasing again."/>
    </annotation>
  </annotations>
</diagram>