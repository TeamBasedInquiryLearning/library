<diagram dimensions="(300,300)" margins="35"  xmlns="https://prefigure.org">
  <definition>f(x)=x^3</definition>
  <coordinates bbox="(-4.1,-60,4.1,60)" >
    <grid-axes hlabels="(-4,1,4)"/>
    <label p="(0,65)"><m>f(x)</m></label>
    <graph at="f" function="f" />
  </coordinates>
  <annotations>
    <annotation ref="figure" text="The graph of the function f of x equals x cubed">
      <annotation ref="f" sonify="yes" text="The function increases then flattens out at the origin, then resumes increasing again"/>
    </annotation>
  </annotations>
</diagram>