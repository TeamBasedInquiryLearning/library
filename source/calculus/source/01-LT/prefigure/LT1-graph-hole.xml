<diagram dimensions="(300,300)" margins="30"  xmlns="https://prefigure.org">
  <definition>f(x)=-x^3+5*x^2-5.5*x</definition>
  <coordinates bbox="(-1,-2,4,4)">
    <grid-axes vlabels="(-1,1,3)"/>
    <label p="(1.5,4.4)"><m>f(x)</m></label>
    <graph at="f" function="f"/>
    <point at="p" p="(2,1)" stroke="blue" fill="white"/>
  </coordinates>

  <annotations>
    <annotation ref="figure" text="The graph of a function with an open circle at a point">
      <annotation ref="f" text="The graph of the function f" sonify="yes"/>
      <annotation ref="p" text="An open circle at the point 2 comma 1"/>
    </annotation>
  </annotations>
</diagram>