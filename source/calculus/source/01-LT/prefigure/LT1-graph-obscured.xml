<diagram dimensions="(300,300)" margins="30" xmlns="https://prefigure.org">
  <definition>f(x)=-x^3+5*x^2-5.5*x</definition>
  <coordinates bbox="(-1,-2,4,4)">
    <grid-axes vlabels="(-1,1,3)"/>
    <label p="(0,4.4)"><m>f(x)</m></label>
    <graph at="f" function="f"/>
    <rectangle at="rectangle" lower-left="(1.9,-3)" dimensions="(0.2,7)" fill="black"/>
  </coordinates>

  <annotations>
    <annotation ref="figure" text="The graph of a function with a portion covered by a black bar">
      <annotation ref="f" text="The graph of the function f" sonify="yes"/>
      <annotation ref="rectangle" text="A narrow black rectangle obscures the graph on either side of the x value 2"/>
    </annotation>
  </annotations>
</diagram>