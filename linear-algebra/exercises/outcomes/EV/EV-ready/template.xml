                <!-- {{#correct}} -->
            <p>({{letter}}) <m>{{item}}</m></p>
                <!-- {{/correct}} -->
            <!-- {{/choices}} -->
        </outtro>
    </knowl>
    <!-- {{/dist_lin_combo}} -->
    <!-- {{#vec_in_set}} -->
    <knowl>
        <content>
            <p>
                Which of the following choices belongs to the
                following set?
                <me>
\left\{ {{vec}} \middle| {{equation}} \right\}
                </me>
            </p>
            <list>
                <!-- {{#choices}} -->
                <item><p>({{letter}}) <m>{{item}}</m></p></item>
