            <p>({{letter}}) <m>{{item}}</m></p>
                <!-- {{/correct}} -->
            <!-- {{/choices}} -->
        </outtro>
    </knowl>
    <!-- {{/vec_in_set}} -->
    <!-- {{#LE1}} -->
