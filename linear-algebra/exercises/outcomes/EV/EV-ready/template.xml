<?xml version='1.0' encoding='UTF-8'?>
<knowl mode="exercise" xmlns="https://spatext.clontz.org" version="0.2">
    <intro><p>Choose the most appropriate response for each question.</p></intro>
    <!-- {{#questions}} -->
    <!-- {{#simp_lin_combo}} -->
    <knowl>
        <content>
            <p>
                Simplify the following Euclidean vector
                expression.
                <me>
{{a}}{{v}} + {{b}}{{w}}
                </me>
            </p>
            <list>
                <!-- {{#choices}} -->
                <item><p>({{letter}}) <m>{{item}}</m></p></item>
                <!-- {{/choices}} -->
            </list>
        </content>
        <outtro>
            <!-- {{#choices}} -->
                <!-- {{#correct}} -->
            <p>({{letter}}) <m>{{item}}</m></p>
                <!-- {{/correct}} -->
            <!-- {{/choices}} -->
        </outtro>
    </knowl>
    <!-- {{/simp_lin_combo}} -->
    <!-- {{#dist_lin_combo}} -->
    <knowl>
        <content>
            <p>
                Simplify the following Euclidean vector
                expression.
                <me>
{{a}}\left({{v}} + {{w}}\right)
