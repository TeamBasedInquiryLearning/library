    <!-- {{#LE3-4}} -->
    <knowl>
        <content>
            <p>
                Find the solution set for
                <me>
{{system}}
                </me>
            </p>
            <list>
                <!-- {{#choices}} -->
                <item><p>({{letter}}) <m>{{item}}</m></p></item>
                <!-- {{/choices}} -->
                <item><p>(d) <m>\emptyset</m></p></item>
            </list>
        </content>
        <outtro>
            <!-- {{#choices}} -->
                <!-- {{#correct}} -->
            <p>({{letter}}) <m>{{item}}</m></p>
                <!-- {{/correct}} -->
            <!-- {{/choices}} -->
