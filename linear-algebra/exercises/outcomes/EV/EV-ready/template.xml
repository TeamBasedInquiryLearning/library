            <!-- {{/choices}} -->
        </outtro>
    </knowl>
    <!-- {{/LE1}} -->
    <!-- {{#equivalent_matrix}} -->
    <knowl>
        <content>
            <p>
                Which of the following augmented matrices is equivalent
